<NotepadPlus>
    <UserLang name="PoE Item Filter" ext="">
        <Settings>
            <Global caseIgnored="no" />
            <TreatAsSymbol comment="no" commentLine="yes" />
            <Prefix words1="no" words2="no" words3="no" words4="no" />
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">&quot;00&quot;00</Keywords>
            <Keywords name="Folder+"></Keywords>
            <Keywords name="Folder-"></Keywords>
            <Keywords name="Operators"></Keywords>
            <Keywords name="Comment"> 1 2 0#</Keywords>
            <Keywords name="Words1">Class BaseType Height Width Rarity ItemLevel DropLevel Quality LinkedSockets Sockets SocketGroup</Keywords>
            <Keywords name="Words2">SetTextColor SetBorderColor SetBackgroundColor SetFontSize PlayAlertSound</Keywords>
            <Keywords name="Words3">Show Hide</Keywords>
            <Keywords name="Words4"></Keywords>
			<Keywords name="Words5">&lt;= &gt;= = &lt; &gt;</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="FOLDEROPEN" styleID="12" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="FOLDERCLOSE" styleID="13" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="KEYWORD1" styleID="5" fgColor="0080f0" bgColor="FFFFFF" fontStyle="1" /> 
            <WordsStyle name="KEYWORD2" styleID="6" fgColor="94558D" bgColor="FFFFFF" fontStyle="1" />
            <WordsStyle name="KEYWORD3" styleID="7" fgColor="390096" bgColor="FFFFFF" fontStyle="1" />
            <WordsStyle name="KEYWORD4" styleID="8" fgColor="00f000" bgColor="FFFFFF" fontStyle="0" />
			<WordsStyle name="KEYWORD5" styleID="17" fgColor="000000" bgColor="FFFFFF" fontStyle="1" />
            <WordsStyle name="COMMENT" styleID="1" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="COMMENT LINE" styleID="2" fgColor="7f7f96" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="NUMBER" styleID="4" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="OPERATOR" styleID="10" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="DELIMINER1" styleID="14" fgColor="008000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="DELIMINER2" styleID="15" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="DELIMINER3" styleID="16" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
        </Styles>
    </UserLang>
</NotepadPlus>