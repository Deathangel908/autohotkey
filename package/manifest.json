{
  "name": "PoE trade ahk",
  "author": "Andrew Koidan, https://github.com/Deathangel908",
  "description": "Path Of Exile extension for poe.trade to scream pm",
  "version": "1.1",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "content_scripts": [ {
    "all_frames": false,
    "js": [ "js/manifest.js", "js/vendor.js", "js/app.js" ],
    "css": ["css/app.css"],
    "matches": [ "http://poe.trade/*" ],
    "run_at": "document_end"
  } ],
  "icons": { "128": "icons/icon-128.png" }
}
